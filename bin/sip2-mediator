#!/usr/bin/perl
#:vim:ft=perl
use strict; use warnings;
use SIPTunnel::Mediator;

# TODO CONFIGS
my $config = {
    sip_address => '127.0.0.1',
    sip_port => 6001,
    http_address => '10.0.0.58',
    http_port => 80,
    http_proto => 'http',
    http_path => '/sip2-mediator',
    max_clients => 120
};

my $mediator = SIPTunnel::Mediator->new($config);
$mediator->listen;


